{
  "maxWorkers": 1,
  "testEnvironment": "node",
  "testRunner": "jest-jasmine2",
  "testTimeout": 120000,
  "reporters": ["detox/runners/jest/streamlineReporter"],
  "verbose": true,
  "preset": "ts-jest",
  "setupFilesAfterEnv": ["./init.ts"],
  "roots": ["../src/modules"],
  "testMatch": ["**/?(*.e2e.)+(spec|test).ts?(x)"]
}
